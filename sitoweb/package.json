{
  "name": "sitoweb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && gzipper --verbose ./dist/sitoweb",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "firebase-deploy": "ng build --prod && gzipper --verbose ./dist/sitoweb && firebase deploy",
    "firebase-serve": "ng build --prod && gzipper --verbose ./dist/sitoweb && firebase serve",
    "analyzer": "ng build --prod --stats-json && webpack-bundle-analyzer dist/sitoweb/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-next.9",
    "@angular/cdk": "^8.2.2",
    "@angular/common": "^9.0.0-next.9",
    "@angular/compiler": "^9.0.0-next.9",
    "@angular/core": "^9.0.0-next.9",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "^9.0.0-next.9",
    "@angular/material": "^8.2.2",
    "@angular/platform-browser": "^9.0.0-next.9",
    "@angular/platform-browser-dynamic": "^9.0.0-next.9",
    "@angular/pwa": "^0.802.2",
    "@angular/router": "^9.0.0-next.9",
    "@angular/service-worker": "^9.0.0-next.9",
    "@ngx-pwa/local-storage": "^8.2.3",
    "firebase": "^6.6.2",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.8",
